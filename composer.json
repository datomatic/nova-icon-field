{
    "name": "datomatic/nova-icon-field",
    "version": "1.0.0",
    "description": "A icon selector field for Laravel Nova 4",
    "keywords": [
        "laravel",
        "nova",
        "nova 4",
        "icon",
        "icons",
        "svg",
        "fontawesome",
        "heroicons"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0|^8.1|^8.2",
        "laravel/nova": "^4.0",
        "outl1ne/nova-translations-loader": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Datomatic\\NovaIconField\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Datomatic\\NovaIconField\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
